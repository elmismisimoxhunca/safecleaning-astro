  <main>
    <section style="background-image: linear-gradient(rgba(9, 34, 65, 0.85), rgba(9, 34, 65, 0.85)), url(/images/SAFE-CLEANING-SERVICIOS-10.webp); background-position: center; background-size: cover; padding: 100px 20px; text-align: center;">
